<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
  <extension point="org.eclipse.ui.perspectiveExtensions">
    <perspectiveExtension targetID="org.talend.rcp.perspective">
      <actionSet id="org.talend.debug.ui.launchActionSet" />
      <actionSet id="org.talend.repository.actionSet" />
      <actionSet id="org.talend.repository.localprovider.actionSet" />
      <actionSet id="org.talend.repository.demoprojcet.actionSet" />
    </perspectiveExtension>
    <perspectiveExtension targetID="*">
      <hiddenToolBarItem id="opengroovyprojectwizard" />
    </perspectiveExtension>
  </extension>
  <extension point="org.eclipse.ui.commands">
    <command
      id="org.talend.rcp.command.closeIntro"
      name="name"
    />
  </extension>
  <extension point="org.eclipse.ui.contexts">
    <context
      id="talend.global"
      name="Default"
      parentId="org.eclipse.ui.contexts.window"
    />
  </extension>
  <extension point="org.talend.core.talendperspectives">
    <TalendPerspective
      refPerspectiveId="org.eclipse.debug.ui.DebugPerspective"
      talendPerspectiveId="org.talend.rcp.DebugPerspective"
    />
  </extension>
  <extension point="org.talend.core.runtime.service">
    <Service
      class="org.talend.rcp.RcpServices"
      serviceId="IRcpService"
    />
    <Service
      class="org.talend.rcp.intro.ApplicationWorkbenchWindowAdvisor"
      serviceId="WorkbenchService"
    />
  </extension>
  <extension point="org.eclipse.ui.bindings">
    <key
      commandId="org.talend.rcp.command.closeIntro"
      schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
      sequence="ESC"
    />
  </extension>
  <extension point="org.eclipse.ui.editors">
    <editor
      class="org.talend.rcp.intro.starting.StartingBrowser"
      default="false"
      id="org.talend.rcp.intro.starting.StartingBrowser"
      name="starting"
    />
  </extension>
  <extension point="org.eclipse.ui.activities">
    <activity
      id="org.talend.rcp.hideUpdatesite"
      name="Hide Updatesite"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.hideUpdatesite"
      isEqualityPattern="true"
      pattern="org.eclipse.ui.ide.application/org.eclipse.update.ui.updateMenu"
    />
  </extension>
  <extension point="org.eclipse.ui.splashHandlers">
    <splashHandler
      class="org.talend.rcp.TalendSplashHandler"
      id="org.talend.rcp.splashHandler"
    />
  </extension>
  <extension point="org.eclipse.ui.commands">
    <command
      id="org.talend.rcp.show.exportLogs.command"
      name="%SupportLogs"
    />
    <command
      id="org.talend.rcp.show.contactSupport.command"
      name="%ContactSupport"
    />
    <command
      id="org.talend.rcp.show.support.menus.command"
      name="%Support"
    />
  </extension>
  <extension point="org.eclipse.ui.handlers">
    <handler
      class="org.talend.rcp.handler.ContactSupportHandler"
      commandId="org.talend.rcp.show.contactSupport.command"
    />
    <handler
      class="org.talend.rcp.handler.ShowExportLogsWizardHandler"
      commandId="org.talend.rcp.show.exportLogs.command"
    />
  </extension>
  <extension point="org.eclipse.ui.menus">
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=additions"
    >
      <menu
        id="talend"
        label="talend"
      />
    </menuContribution>
    <menuContribution
      allPopups="false"
      locationURI="menu:help?before=talend"
    >
      <separator
        name="org.talend.help.spearator.start"
        visible="true"
      />
    </menuContribution>
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=talend"
    >
      <separator
        name="org.talend.help.spearator.end"
        visible="true"
      />
    </menuContribution>
  </extension>
  <extension point="org.eclipse.ui.menus">
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=additions"
    >
      <menu
        id="tdq"
        label="tdq"
      />
    </menuContribution>
    <menuContribution
      allPopups="false"
      locationURI="menu:help?before=tdq"
    >
      <separator
        name="org.talend.help.tdq.spearator.start"
        visible="true"
      />
    </menuContribution>
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=tdq"
    >
      <separator
        name="org.talend.help.tdq.spearator.end"
        visible="true"
      />
    </menuContribution>
  </extension>
  <extension point="org.eclipse.ui.handlers">
    <handler
      class="org.talend.rcp.handler.ShowAboutWizardHandler"
      commandId="org.talend.rcp.show.about.command"
    />
  </extension>
  <extension point="org.eclipse.ui.menus">
    <!--
    <menuContribution allPopups="false" locationURI="menu:help?after=Guided">
      <command
        commandId="org.talend.rcp.plans.command"
        id="org.talend.rcp.plans.command"
        label="%Plans"
        icon="icons/plans.png"
        style="push"
      />
    </menuContribution>
    -->
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=Guided"
    >
      <command
        commandId="org.eclipse.equinox.p2.ui.sdk.install"
        icon="icons/install.png"
        label="%Install"
        style="push"
      />
      <command
        commandId="org.talend.rcp.show.contactSupport.command"
        label="%ContactSupport"
        icon="icons/support.png"
        style="push"
      />
      <!--
      <command
        commandId="org.talend.rcp.whats.new.command"
        id="org.talend.rcp.whats.new.command"
        label="%WhatsNew"
        icon="icons/whats_new.png"
        style="push"
      />
      -->
      <command
        commandId="org.talend.rcp.show.about.command"
        id="org.talend.rcp.show.about.menus.command"
        label="%AboutTalendStudio"
        icon="icons/about.png"
        style="push"
      />
    </menuContribution>
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=Guided"
    >
      <separator
        name="org.talend.help.guide.spearator.end"
        visible="true"
      />
    </menuContribution>
  </extension>
  <extension point="org.eclipse.ui.menus">
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=additions"
    >
      <menu
        id="Academy"
        label="Academy"
      />
    </menuContribution>
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=additions"
    >
      <command 
        commandId="org.talend.resoruses.toolbar.Learn"
        icon="icons/academy.png"
        id="org.talend.resoruses.toolbar.Learn"
        style="push"
      />
    </menuContribution>
    <menuContribution
      allPopups="false"
      locationURI="menu:help?after=additions"
    >
      <command 
        commandId="org.talend.resoruses.toolbar.Ask"
        icon="icons/ask.png"
        id="org.talend.resoruses.toolbar.Ask"
        style="push"
      />
    </menuContribution>
  </extension>
  <extension point="org.eclipse.ui.activities">
    <activity
      id="org.talend.rcp.branding.activity.hideMenu"
      name="hideMenus"
    />
    <activity
      id="org.talend.rcp.branding.activity.hideView"
      name="hideViews"
    />
    <activity
      id="org.talend.rcp.branding.activity.hidePerspective"
      name="hidePerspective"
    />
    <activity
      id="org.talend.rcp.branding.activity.hideImportMenus"
      name="hideImportMenus"
    />
    <activity
      id="org.talend.rcp.branding.activity.hideDebugModules"
      name="hideDebugModules"
    />
    <activity
      id="org.talend.rcp.branding.activity.hideMenuForTOS"
      name="hideMenuForTOS"
    >
      <enabledWhen>
        <test property="org.talend.rcp.booleanTOSTester.isTOS" />
      </enabledWhen>
    </activity>
    <activity
      id="org.talend.rcp.branding.activity.hideViewsPostWindowOpen"
      name="hideViewsPostWindowOpen"
    >
      <enabledWhen>
        <and>
          <test
            forcePluginActivation="true"
            property="org.talend.rcp.languageUseJavaTest.isUseJava"
          />
        </and>
      </enabledWhen>
    </activity>
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      isEqualityPattern="true"
      pattern="org.talend.repository/org.talend.repository.bootTalendAction"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.ui.editors/org.eclipse.ui.edit.text.delimiter.*"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.ui.editors/org.eclipse.ui.edit.text.goto.*"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.ui.ide/goToResource"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.ui.ide/org.eclipse.ui.OpenWorkspaceFile"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.ui.ide/selectWorkingSets"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.search/org.eclipse.search.Open.*"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.search/org.eclipse.search.Text.*"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.ui.externaltools/org.eclipse.ui.run"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.ui.externaltools/org.eclipse.ui.externaltools.ExternalToolMenuDelegate.*"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenu"
      pattern="org.eclipse.ui.externaltools/org.eclipse.ui.externaltools.OpenExternalToolsConfigurations"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideView"
      pattern=".*/org.eclipse.pde.runtime.RegistryBrowser"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideView"
      pattern=".*/org.eclipse.pde.ui.DependenciesView"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideView"
      pattern=".*/org.eclipse.pde.ui.PluginsView"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideView"
      pattern=".*/org.eclipse.team.sync.views.SynchronizeView"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideView"
      pattern=".*/org.eclipse.team.ui.GenericHistoryView"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideViewsPostWindowOpen"
      pattern=".*/org.epic.core.views.browser.BrowserView"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideViewsPostWindowOpen"
      pattern=".*/org.epic.perleditor.views.ExplainErrorsView"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideViewsPostWindowOpen"
      pattern=".*/org.epic.perleditor.views.PerlDocView"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideViewsPostWindowOpen"
      pattern=".*/org.epic.regexp.views.RegExpView"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideViewsPostWindowOpen"
      pattern=".*/org.eclipse.debug.expressionview"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hidePerspective"
      pattern=".*/org.eclipse.team.ui.TeamSynchronizingPerspective"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideMenuForTOS"
      pattern=".*/org.eclipse.equinox.p2.ui.sdk.install"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideImportMenus"
      pattern="org.eclipse.ui.ide/org.eclipse.ui.wizards.import.FileSystem"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideImportMenus"
      pattern="org.eclipse.ui.ide/org.eclipse.ui.wizards.import.ZipFile"
    />
    <activityPatternBinding
      activityId="org.talend.rcp.branding.activity.hideDebugModules"
      pattern="org.eclipse.debug.ui/org.eclipse.debug.ui.ModuleView"
    />
  </extension>
  <extension point="org.eclipse.core.expressions.propertyTesters">
    <propertyTester
      class="org.talend.rcp.test.LanguageUseJavaTest"
      id="org.talend.rcp.languageUseJavaTest"
      namespace="org.talend.rcp.languageUseJavaTest"
      properties="isUseJava"
      type="java.lang.Object"
    />
    <propertyTester
      class="org.talend.rcp.test.BooleanTOSTester"
      id="org.talend.rcp.booleanTOSTester"
      namespace="org.talend.rcp.booleanTOSTester"
      properties="isTOS"
      type="java.lang.Object"
    />
  </extension>
  <extension point="org.talend.core.ui.showPerspectiveAtStartup">
    <perspective id="org.talend.rcp.perspective" />
  </extension>
  <extension
    id="org.talend.resoruses.toolbar"
    point="org.eclipse.ui.commands"
  >
    <command 
      name="%Learn" 
      id="org.talend.resoruses.toolbar.Learn"
    />
    <command 
      name="%AskCommunity" 			
      id="org.talend.resoruses.toolbar.Ask"
    />
  </extension>
  <extension
    id="org.talend.resourse.handler"
    point="org.eclipse.ui.handlers"
  >
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.resoruses.toolbar.referenceCommand"
    />
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.resoruses.toolbar.Learn"
    />
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.resoruses.toolbar.Documentation"
    />
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.resoruses.toolbar.Ask"
    />
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.resoruses.toolbar.Exchange"
    />
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.resoruses.toolbar.Vidoes"
    />
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.resoruses.toolbar.Cloud"
    />
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.rcp.whats.new.command"
    />
    <handler
      class="org.talend.rcp.action.toolbar.ResourceToolbarHandler"
      commandId="org.talend.rcp.plans.command"
    />
  </extension>
  <extension point="org.eclipse.ui.services">
    <sourceProvider provider="org.talend.rcp.intro.ResourceToolbarProvider">
      <variable
        name="ResourceToolbarProvider.showToolbar"
        priorityLevel="workbench"
      />
      <variable
        name="ResourceToolbarProvider.menuExchange"
        priorityLevel="workbench"
      />
      <variable
        name="ResourceToolbarProvider.menuCloud"
        priorityLevel="workbench"
      />
    </sourceProvider>
    <sourceProvider provider="org.talend.rcp.intro.HelpMenuProvider">
      <variable
        name="HelpMenuProvider.isTis"
        priorityLevel="workbench"
      />
    </sourceProvider>
  </extension>
</plugin>
